{
  "compilerOptions": {
    "declaration": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictNullChecks": false,
    "module": "commonjs",
    "target": "es2015",
    "outDir": "dist",
    "noImplicitAny": false,
    "sourceMap": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "exclude": [
    "node_modules"
  ]
}
