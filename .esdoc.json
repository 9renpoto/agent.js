{
  "source": "./src",
  "destination": "./docs",
  "title": "@userdive/agent API Document",
  "test": {
    "type": "mocha",
    "source": "./test",
    "includes": ["test.js$"]
  },
  "plugins": [
    {
      "name": "esdoc-flow-plugin"
    },
    {
      "name": "esdoc-importpath-plugin",
      "option": {
          "replaces": [
            {"from": "^src/", "to": "lib/"}
          ]
        }
      }
    ]
  }
