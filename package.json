  },
  "scripts": {
    "build": "webpack --profile --json | webpack-bundle-size-analyzer",
    "clean": "rimraf dist coverage",
    "codecov": "nyc report --reporter=lcov > coverage.lcov && codecov -t $CODECOV_TOKEN",
    "lint": "npm-run-all -p lint:*",
    "lint:es": "eslint . --cache",
    "lint:flow": "flow",
    "prebuild": "npm run clean",
    "prepush": "npm test",
    "pretest": "npm run -p clean",
    "reinstall": "npm rm @userdive/events @userdive/logger && npm i file:./@userdive/events file:./@userdive/logger -S --production",
